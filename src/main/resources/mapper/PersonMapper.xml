<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.association.mapper.PersonMapper">

    <resultMap id="BaseResultMap" type="com.example.association.entity.Person">
            <id property="perno" column="perNO" jdbcType="INTEGER"/>
            <result property="appno" column="appNO" jdbcType="INTEGER"/>
            <result property="pername" column="perName" jdbcType="VARCHAR"/>
            <result property="perpas" column="perPas" jdbcType="VARCHAR"/>
            <result property="peradr" column="perAdr" jdbcType="VARCHAR"/>
            <result property="perpho" column="perPho" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        perNO,appNO,perName,
        perPas,perAdr,perPho
    </sql>
</mapper>
